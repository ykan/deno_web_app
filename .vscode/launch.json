  "version": "0.2.0",
  "configurations": [

    {
      "name": "Deno: Run",
      "request": "launch",
      "type": "pwa-node",
      "program": "${workspaceFolder}/main.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--import-map=importMap.json",
        "--unstable",
        "--inspect=127.0.0.1:9494",
        "--allow-all",
      ],
      "attachSimplePort": 9494
    }
  ]
}
